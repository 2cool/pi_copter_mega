



<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cc.dewdrop.ffplayer.Settings" >

    <EditText
        android:id="@+id/e_k0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:ems="10"
        android:inputType="numberDecimal|numberSigned"
        android:text="0"
        android:numeric="signed|decimal">

        <requestFocus />
    </EditText>

    <EditText
        android:id="@+id/e_k1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/e_k0"
        android:ems="10"
        android:inputType="numberDecimal|numberSigned"
        android:text="0"
        android:numeric="signed|decimal" />

    <EditText
        android:id="@+id/e_k3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/e_k1"
        android:ems="10"
        android:inputType="numberDecimal|numberSigned"
        android:text="0"
        android:numeric="signed|decimal" />

    <EditText
        android:id="@+id/e_k4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/e_k3"
        android:ems="10"
        android:inputType="numberDecimal|numberSigned"
        android:numeric="signed|decimal"
        android:text="0" />

    <EditText
        android:id="@+id/e_k5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/e_k4"
        android:ems="10"
        android:inputType="numberDecimal|numberSigned"
        android:text="0"
        android:numeric="signed|decimal" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="K0"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/e_k0"
        android:layout_alignRight="@+id/button"
        android:layout_alignEnd="@+id/button" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="K1"
        android:layout_alignTop="@+id/e_k1"
        android:layout_toRightOf="@+id/e_k1"
        android:singleLine="true"
        android:layout_alignRight="@+id/button"
        android:layout_alignEnd="@+id/button" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="K3"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2"
        android:layout_below="@+id/e_k1"
        android:layout_alignRight="@+id/button"
        android:layout_alignEnd="@+id/button" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="K4"
        android:layout_alignTop="@+id/e_k4"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_alignRight="@+id/textView5"
        android:layout_alignEnd="@+id/textView5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="K5"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3"
        android:layout_below="@+id/e_k4"
        android:layout_alignRight="@+id/textView6"
        android:layout_alignEnd="@+id/textView6" />



    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:id="@+id/e_k6"
        android:layout_below="@+id/e_k5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:inputType="numberDecimal|numberSigned"
        android:numeric="signed|decimal"
        android:text="0" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="K6"
        android:id="@+id/textView6"
        android:layout_toRightOf="@+id/e_k5"
        android:layout_below="@+id/e_k5"
        android:layout_toStartOf="@+id/button"
        android:layout_alignRight="@+id/textView7"
        android:layout_alignEnd="@+id/textView7"
        android:layout_above="@+id/e_k7" />


    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="0"
        android:id="@+id/e_n"

        android:text="0"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="31dp"
        android:layout_alignRight="@+id/e_ka"
        android:layout_alignEnd="@+id/e_ka" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Settints:"
        android:id="@+id/textViewSet"
        android:layout_toRightOf="@+id/e_n"
        android:layout_alignTop="@+id/e_n"
        android:layout_alignBottom="@+id/e_n"
        android:layout_toLeftOf="@+id/button"
        android:layout_toStartOf="@+id/button" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:id="@+id/e_k7"
        android:inputType="numberDecimal|numberSigned"
        android:numeric="signed|decimal"
        android:text="0"
        android:layout_below="@+id/e_k6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:id="@+id/e_k8"
        android:inputType="numberDecimal|numberSigned"
        android:numeric="signed|decimal"
        android:text="0"
        android:layout_below="@+id/e_k7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:id="@+id/e_k9"
        android:inputType="numberDecimal|numberSigned"
        android:numeric="signed|decimal"
        android:text="0"
        android:layout_below="@+id/e_k8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:id="@+id/e_ka"
        android:inputType="numberDecimal|numberSigned"
        android:numeric="signed|decimal"
        android:text="0"
        android:layout_below="@+id/e_k9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="K7"
        android:id="@+id/textView7"
        android:layout_alignTop="@+id/e_k7"
        android:layout_toRightOf="@+id/e_k7"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="K8"
        android:id="@+id/textView8"
        android:layout_below="@+id/e_k7"
        android:layout_toRightOf="@+id/e_k7"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="K9"
        android:id="@+id/textView9"
        android:layout_toRightOf="@+id/e_k9"
        android:layout_alignTop="@+id/e_k9"
        android:layout_alignRight="@+id/textView10"
        android:layout_alignEnd="@+id/textView10" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="K10"
        android:id="@+id/textView10"
        android:layout_alignTop="@+id/e_ka"
        android:layout_toRightOf="@+id/e_ka"
        android:layout_alignRight="@+id/button"
        android:layout_alignEnd="@+id/button" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set"
        android:id="@+id/button"
        android:onClick="sendMessage"
        android:layout_alignBaseline="@+id/e_k5"
        android:layout_alignBottom="@+id/e_k5"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button

        android:text="Exit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/exit_button2"
        android:onClick="exit_set"
        android:layout_alignBaseline="@+id/textView7"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
